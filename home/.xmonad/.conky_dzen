background yes
out_to_console yes
out_to_x no
# Update interval in seconds
update_interval 1
 
TEXT
^i(/home/erb/.xmonad/dzen2/cpu.xbm) ^fg(\#FFFFFF)${cpu}% ^i(/home/erb/.xmonad/dzen2/mem.xbm) ^fg(\#FFFFFF)${memperc}% ^fg(\#ebac54) ${if_existing /proc/net/route eth0} ^i(/home/erb/.xmonad/dzen2/net_wired.xbm) ^fg(\#00aa4a) ^i(/home/erb/.xmonad/dzen2/net_down_03.xbm)${downspeed eth0} ^fg(\#ff3333) ^i(/home/erb/.xmonad/dzen2/net_up_03.xbm)${upspeed eth0} ^fg(\#ebac54) ^i(/home/erb/.xmonad/dzen2/mpd.xbm) ^fg(\#00ffff)${exec rhythmbox-client --print-playing-format "%aa-%at-%tt" | cut -c-70} ^fg(\#ebac54) ^i(/home/erb/.xmonad/dzen2/volume.xbm) ^fg(\#00ffff)${exec amixer get Master | egrep -o "[0-9]+%" | head -1 | egrep -o "[0-9]*"}% ^fg(\#ebac54) ^i(/home/erb/.xmonad/dzen2/clock.xbm) ^fg(\#FFFFFF) ${time %d %m %Y} ^fg(\#ebac54) ${time %R}
